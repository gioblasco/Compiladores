PROGRAM errosem08

BEGIN

  INT x, y, z;

  -- Imprime o módulo da subtração entre dois números. ( | A - B | )
  FUNCTION INT MAIN()
  BEGIN
    READ(x);
    READ(y);

    z := x - y;

    printf(x, y);

    IF (z < 0) THEN
      z := z * (0 - 1);
    ENDIF

    WRITE(z);

    RETURN 0;

  END

END
